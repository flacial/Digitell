{
  "version": "0.8.0",
  "name": "Digitell",
  "author": "Void x Flacial",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "type": "module",
    "test": "jest"
  },
  "build": {
    "appId": "com.digitell.app",
    "compression": "store",
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*",
      "assets/"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.1.0",
    "@expo-google-fonts/vt323": "^0.1.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components-react-native": "^5.1.1",
    "expo": "~41.0.1",
    "expo-app-loading": "^1.0.3",
    "expo-av": "~9.1.2",
    "expo-font": "~9.1.0",
    "expo-keep-awake": "~9.1.2",
    "expo-notifications": "~0.11.6",
    "expo-splash-screen": "~0.10.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.5.4",
    "jest": "^27.0.5",
    "prompt-sync": "^4.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-confirmation-code-field": "^7.1.0",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.1.0",
    "react-native-screens": "~3.0.0",
    "react-native-svg": "12.1.0",
    "react-native-unimodules": "~0.13.3",
    "react-native-web": "~0.13.12",
    "react-redux": "^7.2.4",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.12.82",
    "@types/react": "~16.9.35",
    "@types/react-native": "^0.64.10",
    "@types/styled-components": "^5.1.10",
    "electron": "^6.0.12",
    "electron-builder": "^22.11.7",
    "react-native-bundle-visualizer": "^2.2.1",
    "typescript": "~4.0.0"
  },
  "private": true
}
